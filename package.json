{
  "name": "ingens",
  "module": "index.js",
  "type": "module",
  "scripts": {
    "dev": "bun --bun dev.js",
    "dev:watch": "bun --watch --bun index.js",
    "start": "NODE_ENV=production bun --bun index.js",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr client/entry-server.tsx",
    "build": "bun run build:client && bun run build:server"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^9.12.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.3",
    "vite": "^5.4.8"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "compression": "^1.7.4",
    "discord.js": "^14.16.3",
    "express": "^5.0.1",
    "mongoose": "^8.7.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sirv": "^3.0.0"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  }
}
